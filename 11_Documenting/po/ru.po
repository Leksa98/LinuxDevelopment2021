#: guess.c:54
#, c-format
msgid "The program guesses the number from 1 to 100\n"
msgstr "Программа угадывает число от 1 до 100\n"

#: guess.c:55
#, c-format
msgid "Usage: ./guess [-r]\n"
msgstr "Использование: ./guess [-r]\n"

#: guess.c:73
#, c-format
msgid "Guess a number between from I to L\n"
msgstr "Загадайте число от I до L\n"

#: guess.c:75
#, c-format
msgid "Guess a number between from 1 to 100\n"
msgstr "Загадайте число от 1 до 100\n"

#: guess.c:86
#, c-format
msgid "The number is %s!\n"
msgstr "Это число %s!\n"

#: guess.c:88
#, c-format
msgid "The number is %d!\n"
msgstr "Это число %d!\n"

#: guess.c:93
#, c-format
msgid "Is the number bigger than %s? (yes/no)\n"
msgstr "Число больше %s? (да/нет)\n"

#: guess.c:95
#, c-format
msgid "Is the number bigger than %d? (yes/no)\n"
msgstr "Число больше %d? (да/нет)\n"

#: guess.c:98
msgid "yes"
msgstr "да"

#: guess.c:100
msgid "no"
msgstr "нет"

#: guess.c:104
#, c-format
msgid ""
"Please try again. If your guess number is bigger than %s, print 'yes', "
"otherwise print 'no'\n"
msgstr "Пожалуйста, повторите еще раз. Если загаданное число больше %s, напечатайте 'да', иначе напечатайте 'нет'\n"

#: guess.c:106
#, c-format
msgid ""
"Please try again. If your guess number is bigger than %d, print 'yes', "
"otherwise print 'no'\n"
msgstr "Пожалуйста, повторите еще раз. Если загаданное число больше %d, напечатайте 'да', иначе напечатайте 'нет'\n"
